
config interface 'loopback'
	option device 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option ula_prefix 'fd32:0956:d17b::/48'

config device
	option name 'br-lan'
	option type 'bridge'
	option macaddr '08:55:31:fd:92:cf'
	list ports 'eth0.5'

config interface 'lan'
	option device 'br-lan'
	option proto 'static'
	option ipaddr '192.168.1.1'
	option netmask '255.255.255.0'
	option ip6assign '60'

config device
	option name 'eth1'
	option macaddr '08:55:31:fd:92:ce'

config interface 'wan'
	option device 'eth1'
	option proto 'dhcp'

config interface 'wan6'
	option device 'eth1'
	option proto 'dhcpv6'

config switch
	option name 'switch0'
	option reset '1'
	option enable_vlan '1'

config switch_vlan
	option device 'switch0'
	option vlan '0'

config switch_vlan
	option device 'switch0'
	option vlan '1'

config switch_vlan
	option device 'switch0'
	option vlan '2'

config switch_vlan
	option device 'switch0'
	option vlan '3'

config switch_vlan
	option device 'switch0'
	option vlan '4'

config switch_vlan
	option device 'switch0'
	option vlan '5'
	option ports '1 2 3 4 0t'

config device
	option name 'eth0.1'
	option macaddr '9a:69:ac:20:84:33'

config device
	option name 'eth0.2'
	option macaddr '9a:69:ac:20:84:34'

config device
	option name 'eth0.3'
	option macaddr '9a:69:ac:20:84:35'

config device
	option name 'eth0.4'
	option macaddr '9a:69:ac:20:84:36'

config device
	option name 'eth0.5'
	option macaddr '9a:69:ac:20:84:37'

