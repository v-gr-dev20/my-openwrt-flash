
config interface 'loopback'
	option device 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option ula_prefix 'fd28:7651:a887::/48'

config interface 'lan'
	option device 'br-lan'
	option proto 'static'
	option ipaddr '192.168.1.1'
	option netmask '255.255.255.0'
	option ip6assign '60'

config switch
	option name 'switch0'
	option reset '1'
	option enable_vlan '1'

config switch_vlan
	option device 'switch0'
	option vlan '0'

config switch_vlan
	option device 'switch0'
	option vlan '1'

config switch_vlan
	option device 'switch0'
	option vlan '2'

config switch_vlan
	option device 'switch0'
	option vlan '3'

config switch_vlan
	option device 'switch0'
	option vlan '4'

config switch_vlan
	option device 'switch0'
	option vlan '5'
	option ports '8t 0'

config switch_vlan
	option device 'switch0'
	option vlan '6'
	option ports '8t 1 2 3 4'

config device
	option name 'br-lan'
	option type 'bridge'
	list ports 'eth0.6'

config device
	option name 'eth0.1'
	option macaddr 'c8:60:00:e7:83:01'

config device
	option name 'eth0.2'
	option macaddr 'c8:60:00:e7:83:02'

config device
	option name 'eth0.3'
	option macaddr 'c8:60:00:e7:83:03'

config device
	option name 'eth0.4'
	option macaddr 'c8:60:00:e7:83:04'

config device
	option name 'eth0.5'
	option macaddr 'c8:60:00:e7:83:05'

config device
	option name 'eth0.6'
	option macaddr 'c8:60:00:e7:83:06'

config interface 'wan'
	option device 'eth0.5'
	option proto 'dhcp'

config interface 'wan6'
	option device 'eth0.5'
	option proto 'dhcpv6'

