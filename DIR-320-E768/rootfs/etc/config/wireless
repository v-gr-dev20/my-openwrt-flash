
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option path 'ssb0:3'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option key 'password'\''s changed'
	option encryption 'psk2+tkip+ccmp'
	option ssid 'DIR320-E768'

